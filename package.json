{
  "name": "@aliexme/js-utils",
  "version": "0.4.0",
  "description": "Common JavaScript utilities",
  "private": false,
  "author": "Alexander Smirnov <al.smirnov996@gmail.com>",
  "homepage": "https://github.com/aliexme/js-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliexme/js-utils"
  },
  "license": "MIT",
  "main": "index.js",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install && yarn ts:compile",
    "ts:check": "tsc --noEmit",
    "ts:compile": "tsc -p ./tsconfig.lib.json",
    "lint:check": "eslint --ext .js,.mjs,.ts --ignore-path .gitignore --cache ./",
    "lint:fix": "yarn lint:check --fix",
    "test": "jest",
    "test:coverage": "jest --collectCoverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@aliexme/eslint-config": "0.1.1",
    "@aliexme/ts-types": "0.2.2",
    "@types/jest": "27.5.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "lint-staged": "13.1.2",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  }
}
